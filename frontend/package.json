{
  "name": "quiz-frontend",
  "version": "1.0.1",
  "description": "AI-powered quiz application frontend",
  "type": "module",
  "scripts": {
    "prebuild": "node ../scripts/generate-phrasebook-index.js",
    "postbuild": "node ./scripts/generate-front-version.cjs",
    "dev": "vite",
    "build": "vite build",
    "build:fast": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --ignore-pattern 'src/api/**'",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx --ignore-pattern 'src/api/**' --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .prettierignore",
    "test:e2e": "npx playwright test",
    "generate-types": "orval --config ./orval.config.cjs",
    "test": "vitest --run --reporter=verbose",
    "test:find-hanging": "node debug-tests.cjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.2.2",
    "@mantine/dates": "^8.2.2",
    "@mantine/hooks": "^8.2.2",
    "@mantine/notifications": "^8.2.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.80.10",
    "@testing-library/dom": "^10.4.1",
    "audio-feeder": "^0.5.0",
    "axios": "^1.7.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.24.1",
    "react-select": "^5.10.2",
    "react-syntax-highlighter": "^15.6.6",
    "react-timezone-select": "^3.2.8",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.32",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^16.5.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "prettier": "^3.6.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "prismjs": "^1.30.0"
  }
}
