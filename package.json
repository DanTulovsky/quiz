    "url": "git+https://github.com/username/ai-quiz-app.git"
  },
  "author": "Quiz Team",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.55.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.36.0"
  }
