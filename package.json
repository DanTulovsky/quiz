{
  "name": "ai-quiz-app",
  "version": "1.0.0",
  "description": "AI-powered adaptive language learning quiz application",
  "scripts": {
    "dev": "docker-compose up",
    "generate-types": "node scripts/generate-types.js",
    "generate-schema-types": "node scripts/generate-schema-types.js",
    "validate-api": "node scripts/validate-api-contract.js",
    "setup-test-db": "cd backend && go run cmd/setup-test-db/main.go",
